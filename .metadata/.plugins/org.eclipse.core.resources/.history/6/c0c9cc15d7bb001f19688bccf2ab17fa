package com.example.model;


public class Employe {
    private int id;
    private String nom;
    private double salaire;

    public Employe(int id, String nom, double salaire) {
        this.id = id;
        this.nom = nom;
        this.salaire = salaire;
    }

    public int getId() {
        return id;
    }

    public String getNom() {
        return nom;
    }

    public double getSalaire() {
        return salaire;
    }
}
